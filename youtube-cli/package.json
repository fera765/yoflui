{
	"name": "youtube-cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:e2e": "tsx test-e2e.ts",
		"start": "tsx source/cli.tsx",
		"prompt": "tsx source/cli.tsx --prompt"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "^5.2.1",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-text-input": "^6.0.0",
		"meow": "^11.0.0",
		"openai": "^6.7.0",
		"p-queue": "^9.0.0",
		"play-dl": "^1.9.7",
		"react": "^18.3.1",
		"scrape-youtube": "^2.4.0",
		"youtubei.js": "^16.0.1",
		"yt-search": "^2.13.1",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/preset-react": "^7.18.6",
		"@types/node": "^24.9.2",
		"@vdemedes/prettier-config": "^2.0.1",
		"@vitest/ui": "^4.0.5",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"import-jsx": "^5.0.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vitest": "^4.0.5",
		"xo": "^0.53.1"
	},
	"ava": {
		"environmentVariables": {
			"NODE_NO_WARNINGS": "1"
		},
		"nodeArguments": [
			"--loader=import-jsx"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	}
}
